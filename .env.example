# PostgreSQL Configuration
PG_HOST=localhost
PG_DATABASE=mydb
PG_USER=myuser
PG_PASSWORD=mypassword
PG_PORT=5432

# BigQuery Configuration
BQ_PROJECT_ID=my-gcp-project
BQ_DATASET=my_dataset
BQ_TABLE=my_table

# Google Cloud Credentials (path to service account key)
GOOGLE_APPLICATION_CREDENTIALS=path/to/key.json

# Optional: SQL Query
SQL_QUERY=SELECT * FROM public.users WHERE created_at > '2024-01-01'

# Batch Size (number of rows per batch, default: 10000)
BATCH_SIZE=10000
